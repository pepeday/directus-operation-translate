{
  "name": "directus-operation-translate",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.1",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-operation"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "operation",
